<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.huizhou.receptionbooking.afterLogin.tab1.ViewMeetingConfirmInfoActivity">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <com.huizhou.receptionbooking.common.XTextView
            android:id="@+id/viewMeetingAttendBack"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.8"
            android:background="#1D82EA"
            android:drawableLeft="@mipmap/back"
            android:gravity="center"
            android:paddingLeft="10dip"
            android:paddingRight="10dip"
            android:text="参会情况"
            android:textColor="@color/white"
            android:textSize="@dimen/top_size"/>

    </LinearLayout>

    <!-- ListView控件 -->
    <ListView
        android:id="@+id/viewMeetingAttendDataList"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        />


</LinearLayout>
